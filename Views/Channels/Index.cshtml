@model IEnumerable<TVGuide.Models.Channel>

@{
    ViewData["Title"] = "All Channels";
}
<center><h1>All Channels</h1></center>
<div class="thumbnails">
@foreach (var item in Model) {

    <div class="thumbnail">
           <a asp-action="Details" asp-route-id="@item.Id">     <img src="@Html.DisplayFor(modelItem => item.Logo)" alt="Element1" style="width: 75px;" /></a>
    </div>
}</div>

<style>
.thumbnails {text-align: center;}
.thumbnails .thumbnail {display: inline-block; margin: 0 5px;}
</style>