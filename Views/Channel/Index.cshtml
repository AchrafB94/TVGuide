@model IEnumerable<TVGuide.Models.Channel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<div class="row justify-content-center" >
    @foreach (var item in Model) {
        <div class="col-1">
             <a href="/Channel/Details/@Html.DisplayFor(modelItem => item.id)"><img src="@Html.DisplayFor(modelItem => item.logo)" class="img-thumbnail" style="background-color: lightgrey;"/></a>
        </div>
       
    }
    </div>